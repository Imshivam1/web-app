<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Job List</title>
</head>
<body>
    <h1>Job List</h1>
    <table>
        <thead>
            <tr>
                <th>Title</th>
                <th>Location</th>
                <!-- Add more columns as needed -->
            </tr>
        </thead>
        <tbody>
            <% job_list.forEach(job => { %>
                <tr>
                    <td><%= job.title %></td>
                    <td><%= job.location %></td>
                    <!-- Add more columns as needed -->
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
