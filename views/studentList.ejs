<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Student List</title>
</head>
<body>
    <h1>Student List</h1>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>College</th>
                <th>Status</th>
                <!-- Add more columns as needed -->
            </tr>
        </thead>
        <tbody>
            <% student_list.forEach(student => { %>
                <tr>
                    <td><%= student.name %></td>
                    <td><%= student.college %></td>
                    <td><%= student.status %></td>
                    <!-- Add more columns as needed -->
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
